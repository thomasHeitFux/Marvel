{
  "expo": {
    "name": "marvelApp",
    "slug": "marvelApp",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/marvelico.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/marvel.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.thomasfux.marvelApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
